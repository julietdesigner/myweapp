/**index.wxss**/
.home-container {
  height: 100%;
  display: flex;
  padding-top: 60rpx;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.home-nav {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.home-nav-item {
  width: 50%;
  height: 60rpx;
  font-size: 28rpx;
  color: #fff;
  background-color: #28a4ff;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.home-nav-Tab {
  width: 180rpx;
  height: 60rpx;
  line-height: 60rpx;
  box-sizing: border-box;
  text-align: center;
  border-bottom: 6rpx solid #28a4ff;
}